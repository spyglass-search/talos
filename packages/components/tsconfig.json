{
  "compilerOptions": {
    "outDir": "dist",
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": false,
    "jsx": "react-jsx"
  },
  "include": ["src", "packages/components/components/modal/AddNodeModal.tsx", "packages/components/components", "packages/components/components/nodes/extract.tsx", "packages/components/types/index.tsx", "packages/components/utils/metrics.tsx", "packages/components/workflows/executor.tsx"],
  "exclude": ["node_modules", "dist"]
}
